{
    "bleurgh": {
        "base": {
            "background-color": "rgba(0,0,0,0.3)",
            "fill-color": "rgba(64,64,64,0.8)",
            "border-width": 1,
            "padding": 0,
            "border-color": "rgba(0,0,0,0.4)",
            "margin": 2,
            "text-color": "rgba(255,255,255,0.8)",
            "text-padding": 5,
            "font-family": "assets/fonts/telegrama_render.otf",
            "font-size": 8,
            "align-content": "center",
            "justify-content": "space-around",
            "width": "100%"
        },
        
        "panel": {
            "show-header": true,
            "background-color": "rgba(0,0,0,0.3)",
            "width": 200
        },
        
        "group": {
            "background-color": "rgba(0,0,0,0.2)",
            "padding": 0,
            "margin": 0,
            "margin-bottom": 1,
            "align-content": "space-between",
            "flex-direction": "row",
            "flex-wrap": "wrap",
            "width": 200
        },
        
        "group-header": {
            "height": 20,
            "margin": 0,
            "background-color": "rgba(0,0,0,0.2)",
            "text-color": "rgba(255,255,255,1)",
            "width": 200
        },
        
        "slider": {
            "fill-color": "rgba(64,64,64,0.8)",
            "height": 20,
            "precision": 3,
            "padding": 0,
            "width": "100%",
            "border-radius": 1
        },
        
        "toggle": {
            "height": 24,
            "background-color": "rgba(64,64,64,0.3)",
            "border-color": "rgba(32,32,32,0.4)",
            "fill-color": "rgba(128,128,128,0.7)",
            "type": "radio",
            "border-width": 1
        },
        
        "button": {
            "fill-color": "rgba(128,128,128,0.8)",
            "background-color": "rgba(64,64,64,0.4)",
            "type": "fullsize"
        },
        
        "label": {
            "text-color": "rgba(255,255,255,0.7)",
            "border-color": "transparent"
        },
        
        "graphics": {
            "background-color": "rgba(0,0,0,0.4)"
        },
        
        "value-plotter": {
            "fill-color": "rgba(64,64,64,1)",
            "height": 20,
            "precision": 3
        },
        
        "function-plotter": {
            "fill-color": "rgba(64,64,64,1)",
            "precision": 3
        },
        
        "tabs": {
            "border-color": "rgba(32,32,32,0.4)",
            "fill-color": "rgba(64,64,64,0.8)",
            "height": 10,
            "padding": 10
        }
    }
}